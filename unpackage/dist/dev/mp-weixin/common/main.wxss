/* 表单 */
.uni-form-item {
	width: 100%;
	background: #fff;
}
.uni-form-item .title {
	font-size: 28rpx;
	font-family: PingFangSC-Regular, PingFang SC;
	font-weight: 400;
	color: #3D3F48;
	line-height: 40rpx;
	padding: 30rpx 0rpx 10rpx 24rpx;
}
.uni-input {
	height: 45rpx;
	padding: 15rpx 0;
	line-height: 45rpx;
	margin-left: 24rpx;
	background: #FFF;
	border-bottom: 1rpx solid #E7E7E7;
	flex: 1;
	font-size: 32rpx;
	font-family: PingFangSC-Regular, PingFang SC;
	font-weight: 400;
}
.multiple-line-txt-cut {
	display: -webkit-box;
	overflow: hidden;
	line-height: 18px;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
}
.txt-cut {
	max-width: 100%;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.f-c-c {
	display: flex;
	justify-content: center;
	align-items: center;
}
.f-c-c-c {
	display: flex;
	justify-content: center;
	align-items: center;
	flex-direction: column;
}
.f-s-c-c {
	display: flex;
	justify-content: flex-start;
	align-items: center;
	flex-direction: column;
}
.f-fs-c {
	display: flex;
	justify-content: flex-start;
	align-items: center;
}
.f-c-s-c {
	display: flex;
	justify-content: flex-start;
	align-items: flex-start;
	flex-direction: column;
}
.f-s-c {
	display: flex;
	justify-content: space-between;
	align-items: center
}
.f-s-f {
	display: flex;
	justify-content: space-between;
	align-items: flex-start
}
.f-sa-c {
	display: flex;
	justify-content: space-around;
	align-items: center
}
.f-sa-f {
	display: flex;
	justify-content: space-around;
	align-items: flex-start
}
.f-c-f-c {
	display: flex;
	justify-content: center;
	align-items: flex-end;
	flex-direction: column;
}
.f-f-c-c {
	display: flex;
	justify-content: flex-start;
	align-items: center;
	flex-direction: column;
}
.f-sb-c-c {
	display: flex;
	justify-content: space-between;
	align-items: center;
	flex-direction: column;
}
.f-f-c {
	display: flex;
	justify-content: flex-start;
	align-items: center;
}
/* 图片换行排列 */
.f-f-c-f{
	display: flex;
	justify-content: flex-start;
	align-items: center;
	flex-wrap: wrap;
}
